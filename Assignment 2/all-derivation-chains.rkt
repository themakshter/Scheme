(define derivation-src caddr)
(define filter
(lambda (l)
(cond ((null? l) '())
((equal? 'wasDerivedFrom (caar l))
(cons (car l)
(filter (cdr l))))
(else (filter (cdr l))))))


(define statement-id cadr)
(define derivation-src caddr)
(define derivation-dst cadddr)

(define successors 
  (lambda (entity graph)
    (helper entity (filter graph))))

(define helper
  (lambda (e l)
    (cond((null? l) '())
    ((equal? (derivation-src (car l)) e)
     (cons (car l) (helper e (cdr l))))
    (else (helper e (cdr l))))))


(define all-derivation-chains
  (lambda (entity graph)
    (if (null? (successors entity graph)) '()
    (helper2 (successors entity graph) '() graph))))

(define helper2 
  (lambda (sc l g)
    (cond ((null? sc) (list l))
          ((null? (cdr sc)) (sub-helper (car sc) l g))
          (else (append (sub-helper (car sc) l g) (helper2 (cdr sc) l g))))
    )
  )

(define sub-helper
  (lambda (sc l g)
    (helper2 (successors (derivation-dst sc) g) (append l (list (statement-id sc))) g)
    )
  )


